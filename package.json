{
  "name": "state-router-redux-ractive",
  "version": "1.0.1",
  "description": "If you use abstract-state-router and want to use Redux, this package is for you",
  "main": "es5.js",
  "scripts": {
    "test": "npm run build && node test.js",
    "build": "browserify index.js -t [ babelify --presets [ es2015 ] ] --standalone=\"state-router-redux-ractive\" -o es5.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/state-router-redux-ractive.git"
  },
  "keywords": [
    "abstract-state-router",
    "router",
    "redux",
    "ractive",
    "ractivejs",
    "browserify"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/state-router-redux-ractive/issues"
  },
  "homepage": "https://github.com/TehShrike/state-router-redux-ractive#readme",
  "dependencies": {
    "dom-value": "~1.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "abstract-state-router": "5.9.0",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "tape": "4.4.0",
    "tape-catch": "1.0.4",
    "watchify": "3.7.0",
    "redux": "^3.2.0"
  },
  "jsnext:main": "index.js",
  "browser": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015"
        }
      ]
    ]
  }
}
